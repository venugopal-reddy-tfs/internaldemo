pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello World'
            }
        }
        stage('Hello1') {
            steps {
                echo 'Hello World-2'
            }
        }
         stage('Hello2') {
            steps {
                echo 'Hello World-2'
            }
        }
        
        stage('Differnce between two commits') {
            steps {
                sh 'Hello World-2'
            }
        }
    }
}


